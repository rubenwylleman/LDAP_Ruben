;
; BIND reverse data file for local loopback interface
;
$TTL	3600
@	IN	SOA	dns-z2.z2.ruben.hier admin.z2.ruben.hier. (
			     12		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS    	dns-z2.z2.ruben.hier.
dns-z2.z2.ruben.hier.	IN 	A	192.168.12.100

100	IN	PTR	dns-z2.z2.ruben.hier.
100	IN	PTR	squirrelmail
100	IN	PTR	ldap
100	IN	PTR	www
101	IN	PTR	in
102	IN	PTR	cheese
103	IN	PTR	out


